package gmit.ie;

import java.io.BufferedWriter;
import java.io.File;
import java.io.IOException;

public class FileWriter {

	public void writer(String fileName, String text) throws Exception {
		BufferedWriter writer=null;
		FileWriter fw=null;
		File f=null;
		try {
			f=new File(fileName);
			fw=new FileWriter(f);
			writer=new BufferedWriter(new FileWriter());
		}catch(IOException e) {
			e.printStackTrace();
		}
	}
}
