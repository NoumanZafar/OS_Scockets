package gmit.ie;

import java.io.IOException;
import java.io.ObjectOutputStream;

public class UpdateBug {
	private boolean exit=true;
	
	public UpdateBug() {
		super();
	}
	
	public void update() {
		do {
			
		}while(exit);
	}
	
	public void sendMessage(String message, ObjectOutputStream out) {
		try {
			out.writeObject(message);
			out.flush();
		} catch (IOException e) {
			e.printStackTrace();
		}
	}
}
